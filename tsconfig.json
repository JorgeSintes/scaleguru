{
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"],
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./dist",
    "removeComments": true,
    "sourceMap": true,
    "target": "ES6",
    "moduleResolution": "node",
    "module": "ES6",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "extends": "expo/tsconfig.base"
}
